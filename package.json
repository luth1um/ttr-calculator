{
  "name": "ttr-calculator",
  "version": "2.0.0",
  "private": true,
  "homepage": "/ttr-calculator",
  "dependencies": {
    "bootstrap": "5.3.8",
    "i18next": "25.5.3",
    "i18next-browser-languagedetector": "8.2.0",
    "i18next-http-backend": "3.0.2",
    "react": "19.2.0",
    "react-bootstrap": "2.10.10",
    "react-dom": "19.2.0",
    "react-i18next": "16.0.0",
    "react-scripts": "5.0.1",
    "ttr-calculator-typescript": "1.0.15"
  },
  "devDependencies": {
    "@babel/plugin-transform-private-property-in-object": "7.27.1",
    "@eslint/js": "9.37.0",
    "@types/node": "24.6.2",
    "@types/react": "19.2.0",
    "@types/react-dom": "19.2.0",
    "eslint": "9.37.0",
    "gh-pages": "6.3.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.3",
    "prettier": "3.6.2",
    "react-app-rewired": "2.2.1",
    "rimraf": "6.0.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.45.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "clean": "rimraf build",
    "prebuild": "pnpm clean",
    "build": "BUILD_PATH='./build/ttr-calculator' react-app-rewired build",
    "format": "prettier --write \"./**/*.{css,html,js,json,md,ts,tsx,yml,yaml}\"",
    "lint": "eslint . --fix --max-warnings 0",
    "predeploy": "pnpm build build",
    "deploy": "gh-pages -d build/ttr-calculator",
    "test": "react-app-rewired test",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix --report-unused-disable-directives --max-warnings 0"
    ],
    "*.{css,html,js,json,md,yml,yaml}": "prettier --write"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "core-js",
      "core-js-pure"
    ],
    "overrides": {
      "postcss@<8.4.31": ">=8.4.31",
      "nth-check@<2.0.1": ">=2.0.1"
    }
  }
}
