{
  "name": "ttr-calculator",
  "version": "3.0.0",
  "private": true,
  "homepage": "/ttr-calculator",
  "dependencies": {
    "bootstrap": "5.3.8",
    "i18next": "25.7.1",
    "i18next-browser-languagedetector": "8.2.0",
    "i18next-http-backend": "3.0.2",
    "react": "19.2.1",
    "react-bootstrap": "2.10.10",
    "react-dom": "19.2.1",
    "react-i18next": "16.3.5",
    "ttr-calculator-typescript": "1.0.18"
  },
  "devDependencies": {
    "@eslint/js": "9.39.1",
    "@types/node": "24.10.1",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.1",
    "eslint": "9.39.1",
    "eslint-plugin-react-hooks": "7.0.1",
    "gh-pages": "6.3.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "prettier": "3.7.4",
    "rimraf": "6.1.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.48.1",
    "vite": "7.2.6"
  },
  "scripts": {
    "start": "vite",
    "clean": "rimraf dist",
    "format": "prettier --write \"./**/*.{css,html,js,json,md,ts,tsx,yml,yaml}\"",
    "lint": "eslint . --fix --max-warnings 0",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "deploy": "gh-pages -d dist/ttr-calculator",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix --report-unused-disable-directives --max-warnings 0"
    ],
    "*.{css,html,js,json,md,yml,yaml}": "prettier --write"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "core-js",
      "core-js-pure",
      "esbuild"
    ]
  }
}
