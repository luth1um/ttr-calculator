{
  "name": "ttr-calculator",
  "version": "1.0.7",
  "private": true,
  "homepage": "/ttr-calculator",
  "dependencies": {
    "bootstrap": "5.3.3",
    "i18next": "23.11.5",
    "i18next-browser-languagedetector": "8.0.0",
    "i18next-http-backend": "2.5.2",
    "react": "18.3.1",
    "react-bootstrap": "2.10.3",
    "react-dom": "18.3.1",
    "react-i18next": "14.1.2",
    "react-scripts": "5.0.1",
    "ttr-calculator-typescript": "1.0.10"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "7.21.11",
    "@types/node": "20.14.9",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "eslint": "9.6.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "gh-pages": "6.1.1",
    "husky": "9.0.11",
    "lint-staged": "15.2.7",
    "prettier": "3.3.2",
    "rimraf": "5.0.7",
    "typescript": "5.5.2",
    "typescript-eslint": "7.14.1"
  },
  "resolutions": {
    "follow-redirects": "1.15.6",
    "nth-check": "2.1.1",
    "postcss": "8.4.33",
    "webpack-dev-middleware": "5.3.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "clean": "rimraf build",
    "prebuild": "yarn clean",
    "build": "BUILD_PATH='./build/ttr-calculator' react-scripts build",
    "format": "prettier --write \"./**/*.{css,html,js,json,md,ts,tsx,yml,yaml}\"",
    "lint": "eslint . --fix --max-warnings 0",
    "predeploy": "yarn build build",
    "deploy": "gh-pages -d build/ttr-calculator",
    "test": "react-scripts test",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix --report-unused-disable-directives --max-warnings 0"
    ],
    "*.{css,html,js,json,md,yml,yaml}": "prettier --write"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
