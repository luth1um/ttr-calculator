{
  "name": "ttr-calculator",
  "version": "1.0.7",
  "private": true,
  "homepage": "/ttr-calculator",
  "dependencies": {
    "bootstrap": "5.3.5",
    "i18next": "25.0.0",
    "i18next-browser-languagedetector": "8.0.5",
    "i18next-http-backend": "3.0.2",
    "react": "19.1.0",
    "react-bootstrap": "2.10.9",
    "react-dom": "19.1.0",
    "react-i18next": "15.4.1",
    "react-scripts": "5.0.1",
    "ttr-calculator-typescript": "1.0.14"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "7.21.11",
    "@eslint/js": "9.24.0",
    "@types/node": "22.14.1",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.2",
    "eslint": "9.24.0",
    "eslint-config-prettier": "10.1.2",
    "eslint-plugin-prettier": "5.2.6",
    "gh-pages": "6.3.0",
    "husky": "9.1.7",
    "lint-staged": "15.5.1",
    "prettier": "3.5.3",
    "rimraf": "6.0.1",
    "typescript": "5.8.3",
    "typescript-eslint": "8.30.1"
  },
  "resolutions": {
    "nth-check": "2.1.1",
    "postcss": "8.5.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "clean": "rimraf build",
    "prebuild": "yarn clean",
    "build": "BUILD_PATH='./build/ttr-calculator' react-scripts build",
    "format": "prettier --write \"./**/*.{css,html,js,json,md,ts,tsx,yml,yaml}\"",
    "lint": "eslint . --fix --max-warnings 0",
    "predeploy": "yarn build build",
    "deploy": "gh-pages -d build/ttr-calculator",
    "test": "react-scripts test",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix --report-unused-disable-directives --max-warnings 0"
    ],
    "*.{css,html,js,json,md,yml,yaml}": "prettier --write"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
